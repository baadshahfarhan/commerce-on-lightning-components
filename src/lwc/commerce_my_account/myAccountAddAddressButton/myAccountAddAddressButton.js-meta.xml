<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <isExposed>true</isExposed>
    <targets>
		<target>lightningCommunity__Page</target>
		<target>lightningCommunity__Default</target>
	</targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Page">
            <paletteSection>Commerce</paletteSection>
        </targetConfig>
         <targetConfig targets="lightningCommunity__Default">
            <property label="Label" name="text" type="String" default="Add Address" translatable="true"/>
            <property label="Style" name="variant" type="String" default="primary" datasource="java://siteforce.communitybuilder.datasource.ButtonStyleDataSource"/>
            <property label="Size" name="size" type="String" default="standard" datasource="java://siteforce.communitybuilder.datasource.ButtonSizeDataSource"/>
            <property label="Width" name="width" type="String" default="standard" datasource="java://siteforce.communitybuilder.datasource.ButtonStretchDataSource"/>
            <property label="Align" name="alignment" type="String" default="right"/>
            <designLayout>
                  <designSection name="addAddress">
                     <designLayoutItems>
                        <designLayoutProperty name="text"/>
                        <designLayoutProperty name="variant"/>
                        <designLayoutProperty name="size"/>
                        <designLayoutProperty name="width"/>
                        <designLayoutProperty name="alignment"/>
                    </designLayoutItems>
                  </designSection>
            </designLayout>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>